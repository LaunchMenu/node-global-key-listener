{
    "name": "node-global-key-listener-electron-test",
    "private": true,
    "description": "An electron test for ngkl",
    "types": "build/index.d.ts",
    "main": "build/index.js",
    "version": "0.0.0",
    "files": [
        "bin/**/*",
        "build/**/*"
    ],
    "scripts": {
        "start": "electron build/index.js",
        "watch": "concurrently \"tsc -w\" \"cpx \\\"src/**/*.{html,png,jpg,json}\\\" \\\"build\\\" --watch --verbose\"",
        "build": "tsc",
        "dist": "electron-builder"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LaunchMenu/node-global-key-listener.git"
    },
    "keywords": [
        "key",
        "listener",
        "global",
        "node"
    ],
    "author": "Sancarn",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/LaunchMenu/node-global-key-listener/issues"
    },
    "homepage": "https://github.com/LaunchMenu/node-global-key-listener#readme",
    "devDependencies": {
        "@types/node": "^14.14.37",
        "concurrently": "^6.3.0",
        "cpx": "^1.5.0",
        "electron": "^15.1.2",
        "electron-builder": "^22.13.1",
        "typescript": "^4.2.3"
    },
    "dependencies": {
        "electron-is-running-in-asar": "^1.0.0",
        "node-global-key-listener": "link:.."
    },
    "build": {
        "appId": "com.electron.launchmenu-node-global-key-listener",
        "productName": "node-global-key-listener",
        "files": [
            "build/**/*"
        ],
        "directories": {
            "buildResources": "build",
            "output": "dist",
            "app": "."
        },
        "nsis": {
            "allowElevation": false,
            "allowToChangeInstallationDirectory": true,
            "oneClick": false,
            "perMachine": true
        },
        "win": {
            "requestedExecutionLevel": "requireAdministrator"
        },
        "electronVersion": "10.1.2",
        "extraResources": [
            {
                "from": "../../node_modules/regedit/vbs",
                "to": "regedit/vbs",
                "filter": [
                    "**/*"
                ]
            }
        ]
    }
}
